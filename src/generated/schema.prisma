generator client {
  provider   = "prisma-client-js"
  engineType = "client" // enable Prisma ORM without Rust
  output     = "../src/generated"
}

datasource db {
  provider = "postgresql"
}

model User {
  id Int @id @default(autoincrement())

  fullName String
  email    String
  password String

  createAt  DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Product {
  id Int @id @default(autoincrement())

  name      String
  imageUrl  String
  createAt  DateTime @default(now())
  updatedAt DateTime @updatedAt
}
